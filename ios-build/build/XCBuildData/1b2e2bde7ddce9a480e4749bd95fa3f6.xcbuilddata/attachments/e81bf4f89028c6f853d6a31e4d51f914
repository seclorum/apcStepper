#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/jayvaughan/Documents/Development/Plugins/apcStepper/ios-build
  /opt/homebrew/bin/cmake -E copy /Users/jayvaughan/Documents/Development/Plugins/apcStepper/ios-build/apcStepper_artefacts/JuceLibraryCode/apcStepper_Standalone/PkgInfo /Users/jayvaughan/Documents/Development/Plugins/apcStepper/ios-build/apcStepper_artefacts/Debug/Standalone/apcStepper.app/Contents
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/jayvaughan/Documents/Development/Plugins/apcStepper/ios-build
  /opt/homebrew/bin/cmake -E copy /Users/jayvaughan/Documents/Development/Plugins/apcStepper/ios-build/apcStepper_artefacts/JuceLibraryCode/apcStepper_Standalone/PkgInfo /Users/jayvaughan/Documents/Development/Plugins/apcStepper/ios-build/apcStepper_artefacts/Release/Standalone/apcStepper.app/Contents
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/jayvaughan/Documents/Development/Plugins/apcStepper/ios-build
  /opt/homebrew/bin/cmake -E copy /Users/jayvaughan/Documents/Development/Plugins/apcStepper/ios-build/apcStepper_artefacts/JuceLibraryCode/apcStepper_Standalone/PkgInfo /Users/jayvaughan/Documents/Development/Plugins/apcStepper/ios-build/apcStepper_artefacts/MinSizeRel/Standalone/apcStepper.app/Contents
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/jayvaughan/Documents/Development/Plugins/apcStepper/ios-build
  /opt/homebrew/bin/cmake -E copy /Users/jayvaughan/Documents/Development/Plugins/apcStepper/ios-build/apcStepper_artefacts/JuceLibraryCode/apcStepper_Standalone/PkgInfo /Users/jayvaughan/Documents/Development/Plugins/apcStepper/ios-build/apcStepper_artefacts/RelWithDebInfo/Standalone/apcStepper.app/Contents
fi

